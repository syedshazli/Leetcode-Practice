class Solution(object):
    def twoSum(self, numbers, target):
        left = 0 
        right = len(numbers) - 1
        sum = 0

        while left < right:
            sum = numbers[l] + numbers[r]
            if sum > target:
                right -= 1
            elif sum < target:
                left += 1
            else:
                return [left + 1, right + 1]
